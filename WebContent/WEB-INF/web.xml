<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>WebApp</display-name>
  <servlet>
    <servlet-name>Chat</servlet-name>
    <servlet-class>com.chatting.Chat</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ChatStore</servlet-name>
    <servlet-class>com.chatting.ChatStore</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReloadData</servlet-name>
    <servlet-class>com.chatting.ReloadData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Chat</servlet-name>
    <url-pattern>/chat</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChatStore</servlet-name>
    <url-pattern>/chatstore</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReloadData</servlet-name>
    <url-pattern>/reloaddata</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>